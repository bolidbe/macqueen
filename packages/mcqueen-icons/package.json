{
  "name": "@bolid/mcqueen-icons",
  "version": "0.1.0",
  "description": "A scalable set of icons handcrafted with <3 by bolid.",
  "homepage": "https://github.com/bolidbe/mcqueen/tree/master/packages/mcqueen-icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolidbe/mcqueen.git"
  },
  "bugs": {
    "url": "https://github.com/bolidbe/mcqueen/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Gabriel Sebag <gabriel@bolid.be>",
  "license": "Apache-2.0",
  "keywords": [
    "bolid",
    "icons",
    "svg",
    "mcqueen",
    "react"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:svgs && npm run build:react && npm run build:rollup",
    "build:svgs": "scripts/svgs.js --input icons/**/*.svg --output src/__generated__/data.json",
    "build:react": "scripts/react.js",
    "build:rollup": "rollup -c",
    "prepublishOnly": "yarn build",
    "test": "echo \"No test specified\""
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^6.0.1",
    "globby": "^11.0.0",
    "lodash.merge": "^4.6.2",
    "react": "^16.4.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.5",
    "rollup-plugin-commonjs": "^9.1.3",
    "trim-newlines": "^3.0.0",
    "typescript": "^3.7.5",
    "yargs": "^15.1.0"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "engines": {
    "node": ">=8"
  }
}
