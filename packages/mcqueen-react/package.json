{
  "name": "@bolid/mcqueen-react",
  "version": "1.4.7",
  "description": "React components for building UIs with McQueen.",
  "homepage": "https://github.com/bolidbe/mcqueen/tree/master/packages/mcqueen-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolidbe/mcqueen.git"
  },
  "bugs": {
    "url": "https://github.com/bolidbe/mcqueen/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "bolid <tech@bolid.be>",
  "license": "Apache-2.0",
  "keywords": [
    "bolid",
    "mcqueen",
    "react"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "babel.config.js",
    "tsconfig.json"
  ],
  "sideEffects": false,
  "scripts": {
    "clear": "rm -rf dist",
    "dev": "npm run clear && rollup -cw",
    "build": "npm run clear && rollup -c && replace-in-files --string='@bolid/mcqueen-icons' --replacement='@bolid/mcqueen-icons/dist/es' 'dist/es/components/**/*' && replace-in-files --regex='../src/components/' --replacement='./' 'dist/es/components/**/*'",
    "prepublishOnly": "npm run build",
    "test": "echo \"No test specified\"",
    "typecheck": "tsc -w"
  },
  "dependencies": {
    "focus-trap": "4.0.2",
    "intersection-observer": "0.11.0",
    "no-scroll": "2.1.1",
    "object-fit-images": "3.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.7",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@bolid/mcqueen-scss": "^1.1.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@types/classnames": "2.2.11",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.10",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.0",
    "@types/warning": "3.0.0",
    "autoprefixer": "9.8.6",
    "cssnano": "4.1.10",
    "postcss-import": "12.0.0",
    "replace-in-files-cli": "1.0.0",
    "rollup": "2.33.3",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "sass": "1.26.11",
    "tailwindcss": "1.9.6",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@bolid/mcqueen-icons": ">= 1.1.4",
    "@bolid/mcqueen-scss": ">= 1.1.1",
    "@popperjs/core": ">= 2.5.4 < 3.0.0",
    "classnames": ">= 2.2.6 < 3.0.0",
    "html-react-parser": ">= 0.14.2",
    "lodash": ">= 4.17.20 < 5.0.0",
    "query-string": ">= 6.13.5 < 7.0.0",
    "react": ">= 16.12.0 < 17.0.0-0",
    "react-autosuggest": ">= 10.1.0",
    "react-dom": ">= 16.12.0 < 17.0.0-0",
    "react-popper": ">= 2.2.4 < 3.0.0",
    "warning": ">= 4.0.3 < 5.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
