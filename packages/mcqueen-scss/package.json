    "test": "echo \"No test specified\""
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "cssnano": "^4.1.0",
    "node-sass": "^4.9.2",
    "postcss-cli": "^6.1.1",
    "tailwindcss": "^1.1.3"
  },
  "engines": {
    "node": ">=8"
  }
}
